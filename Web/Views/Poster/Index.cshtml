<script>
    document.addEventListener("DOMContentLoaded", function (event) {
        if (navigator.userAgent.toLowerCase().indexOf("android") > -1) {
            window.location.href = 'https://play.google.com/store/apps/details?id=app.travelorientalmindoro.ph';
        }
        else if (navigator.userAgent.toLowerCase().indexOf("iphone") > -1) {
            window.location.href = 'https://apps.apple.com/us/app/travel-oriental-mindoro-ph/id1570131703';
        }
        else {
            window.location.href = 'https://travelorientalmindoro.ph/';
        }
    });
</script>